            <div class="views-exposed-wrapper">
                  <div class="views-exposed-wrapper__form">
            <form class="views-exposed-form bef-exposed-form usa-form" data-drupal-selector="views-exposed-form-content-listing-relationships-content-listing-topic-type" action="/news/rss-feed/41127" method="get" id="views-exposed-form-content-listing-relationships-content-listing-topic-type" accept-charset="UTF-8">
          <div class="form-elements-wrapper" data-filter-count="2">
      <div class="usa-form-group js-form-item form-item js-form-type-select form-item-topic-id js-form-item-topic-id">
        <label for="edit-topic-id--3" class="form-label"><span class="form-label__title">Topic</span></label>
          <div class="select-wrapper" data-select-name="topic_id"><select data-replace-label="1" data-drupal-selector="edit-topic-id" multiple name="topic_id[]" id="edit-topic-id--3" size="4" class="form-select form-select-multiple" data-label="Topic" data-width="100%"><option value="">Topic</option><option value="34283">Civics (2008 version)</option><option value="42158">Civics (2025 Version)</option><option value="34280">English</option><option value="33600">Citizenship and Naturalization</option></select></div>
      </div>
<div class="usa-form-group js-form-item form-item js-form-type-select form-item-doc-type js-form-item-doc-type">
        <label for="edit-doc-type--3" class="form-label"><span class="form-label__title">Type</span></label>

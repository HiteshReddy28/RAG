      <div class="container container--main" lang="ht">
                

                        
            <h1 class="page-title js-page-title">
              Enfòmasyon Siplemantè sou Depo pou Anilasyon Frè            </h1>
          
              
                    
          <div class="translations-section">
      <div class="views-element-container">

<div class="container container--views content-translations-links default content-translations-links-default js-view-dom-id-8bc14becbc798fd3f5592155cbd6fa5d7652a89b8166de7f424ee6ae6f69ce6e hide-view-label">
  
  
  

  
      
  
        
  <div class="views-scroll-marker"></div>
      <div class="translations popup-root" dir="ltr">  <ul class="translation-links">
          <li class="translation-link translation-link--en">
        <a href="https://www.uscis.gov/forms/filing-fees/additional-information-on-filing-a-fee-waiver" lang="en" dir="ltr">English</a>
      </li>
      </ul>

        <ul class="translation-links view--link">      <li class="translation-link popup-content__menu-item">
            
      <a href="/forms/filing-fees/additional-information-on-filing-a-fee-waiver-dari" dir="rtl" class="lang-link" lang="fa-AF">دری</a>
  </li>
      <li class="translation-link popup-content__menu-item">
            
      <span class="lang-text lang-text--plain popup-content__menu-item__text" lang="ht">Kreyòl Ayisyen</span>
  </li>
</ul>
      <a href="#language-toggle" class="language-toggle js-popup-toggle" data-ind-down="fa-chevron-down" data-ind-up="fa-chevron-up">
